<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record
            id="todo_app_task_owner"
            model="ir.rule"
        >
            <field name="name">Todo task for owner</field>
            <field name="model_id" ref="model_todo_app_task"></field>
            <field name="domain_force">[('create_uid', '=', user.id), ('priority', '=', 'low')]</field>
            <field
                name="groups"
                eval="[(4, ref('base.group_user'))]"
            />
        </record>
    </data>
</odoo>
